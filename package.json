{
  "name": "safe-any",
  "version": "0.0.1",
  "description": "Safe interface to JSON and any objects written in TypeScript.",
  "main": "./out-tsc/index.js",
  "types": "./out-tsc/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "test": "npm run build && npm run build-dist && nyc mocha",
    "coverage": "nyc --reporter=text-lcov mocha | coveralls",
    "docs": "typedoc --out docs ./lib",
    "postinstall": "npm run build"
  },
  "keywords": [
    "json",
    "typescript",
    "any"
  ],
  "author": "Samuel Schepp",
  "license": "MIT",
  "dependencies": {
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/assert": "^1.4.6",
    "@types/mocha": "^5.2.7",
    "assert": "^2.0.0",
    "coveralls": "^3.0.11",
    "mocha": "^6.2.3",
    "nyc": "^14.1.1",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.1"
  }
}
