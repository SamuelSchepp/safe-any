{
  "name": "safe-any",
  "version": "0.0.1",
  "description": "Safe interface to JSON and any objects written in TypeScript.",
  "main": "./out-tsc/index.js",
  "types": "./out-tsc/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint index.ts lib/**/*.ts test/**/*.ts",
    "test": "nyc mocha",
    "coverage": "nyc --reporter=text-lcov mocha | coveralls",
    "docs": "typedoc --out docs ./lib",
    "postinstall": "npm run build"
  },
  "keywords": [
    "json",
    "typescript",
    "any"
  ],
  "author": "Samuel Schepp",
  "license": "MIT",
  "dependencies": {
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/assert": "^1.4.6",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "assert": "^2.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-explicit-typescript": "git+https://github.com/SamuelSchepp/eslint-config-explicit-typescript.git#1.0.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "ts-node": "^8.10.1",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.1"
  }
}
