{
  "name": "typed-any-access",
  "version": "1.1.0",
  "description": "Safe interface to JSON and any objects written in TypeScript.",
  "main": "./out-tsc/index.js",
  "types": "./out-tsc/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint index.ts lib/**/*.ts test/**/*.ts",
    "test": "nyc mocha",
    "coverage": "nyc --reporter=lcovonly mocha",
    "docs": "typedoc --out docs ./lib"
  },
  "keywords": [
    "json",
    "typescript",
    "any"
  ],
  "files": [
    "out-tsc/**",
    "package.json",
    "package-lock.json",
    "README.md",
    "LICENSE"
  ],
  "author": "Samuel Schepp",
  "license": "MIT",
  "repository": "https://github.com/SamuelSchepp/typed-any-access",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/assert": "^1.4.6",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "assert": "^2.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.2.0",
    "eslint-config-explicit-typescript": "^1.3.2",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "ts-node": "^8.10.1",
    "typedoc": "^0.15.1",
    "typescript": "^3.8.0"
  }
}
