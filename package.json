{
  "name": "typed-any-access",
  "version": "1.1.1-rc1",
  "description": "Safe interface to JSON and any objects written in TypeScript.",
  "main": "./out-tsc/index.js",
  "types": "./out-tsc/index.d.ts",
  "scripts": {
    "build": "rimraf out-tsc && tsc",
    "lint": "eslint index.ts lib/**/*.ts test/**/*.ts",
    "test": "nyc mocha",
    "coverage": "nyc --reporter=lcovonly mocha",
    "docs": "rimraf docs && typedoc --out docs ./lib"
  },
  "keywords": [
    "json",
    "typescript",
    "any"
  ],
  "files": [
    "out-tsc/**",
    "package.json",
    "package-lock.json",
    "README.md",
    "LICENSE"
  ],
  "author": "Samuel Schepp",
  "license": "MIT",
  "repository": "https://github.com/SamuelSchepp/typed-any-access",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/assert": "^1.5.2",
    "@types/mocha": "^8.0.4",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "assert": "^2.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "eslint-config-explicit-typescript": "^1.3.2",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  }
}
